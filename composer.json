{
    "name": "webthink/codesniffer",
    "homepage": "https://github.com/webthinkgr/codesniffer",
    "description": "This package contains my personal CodeStyle rules",
    "keywords": [
        "review",
        "sniff"
    ],
    "authors": [
        {
            "name": "George Mponos",
            "email": "gmponos@gmail.com",
            "homepage": "https://gmponos.webthink.gr",
            "role": "web developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "squizlabs/php_codesniffer": "3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "WebthinkSniffer\\": "src/Webthink"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebthinkSniffer\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
